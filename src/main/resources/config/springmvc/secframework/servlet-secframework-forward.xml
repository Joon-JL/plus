<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<!-- 공통코드 관리페이지로 Forwarding -->
	<bean id="secfw.codeForward"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/secfw/code/CodeMngList.jsp" />
	</bean>
	
	<!-- 사용회사코드 관리페이지로 Forwarding -->
	<bean id="secfw.codeCompForward"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/secfw/code/CodeCompMngList.jsp" />
	</bean>
	
	<!-- SSO CHECK 페이지로 Forwarding -->
	<bean id="secfw.ssoCheckPageForward"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/secfw/user/SSOCheckFw.jsp" />
	</bean>
	
	<!-- 2014-05-14 Kevin added. -->
	<!-- Forward to SSO check page for external system -->
	<bean id="secfw.ssoCheckPageForwardForExternal" class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/secfw/user/SSOCheckForExternal.jsp" />
	</bean>		
	
	<!-- SSO CHECK 페이지로 Forwarding -->
	<bean id="secfw.ssoPageForward"
		class="anyframe.web.springmvc.controller.ForwardController">
		<!-- Mail 등 일부에서 LoginPage를 거치지 않을경우. 2011/04/07 추가 -->
		<property name="viewName" value="/WEB-INF/jsp/secfw/user/SSOForward.jsp" />		
	</bean>
	
	<!-- SSO CHECK 페이지(크로스 브라우징)로 Forwarding -->
	<bean id="secfw.ssoLoginCrossBrowse"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/secfw/user/ssoLoginCrossBrowse.jsp" />
	</bean>


	<!-- Intro 페이지로 Forwarding -->
	<bean id="secfw.introPageForward"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/secfw/main/ConnectInfo.jsp" />
	</bean>	
			
	<!-- ################################################################ -->
	<!-- ###### 메인페이지 Template #####-->
	<!-- ################################################################ -->
	<!-- 메인페이지로 Forwarding -->
	<bean id="secfw.mainPageForward"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/template/TemplateMain.jsp" />
	</bean>

	<bean id="secfw.mainTemplateTopForward"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/template/TemplateTop.jsp" />
	</bean>

	<bean id="secfw.mainTemplateLeftForward"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/template/left/AdminLeft.jsp" />
	</bean>

	<bean id="secfw.mainTemplateContentForward"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/template/content/TemplateMainContent.html" />
	</bean>

	<!-- ################################################################ -->
	<!-- ###### Volatile												  -->
	<!-- ################################################################ -->
	<bean id="secfw.volatile.login"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/secfw/volatile/LoginPage.jsp" />
	</bean>
	
	
	<!-- ################################################################ -->
	<!-- ###### 게시판 에러(개발보류함)										  -->
	<!-- ################################################################ -->
	<!-- 삭제된 게시판 -->
	<bean id="secfw.board.deletedBoard"
		class="anyframe.web.springmvc.controller.ForwardController">
		<property name="viewName" value="/WEB-INF/jsp/secfw/error/ErrorBoard.jsp" />
	</bean>	

  <!-- dext Sample Forwarding -->
  <bean id="secfw.dextForward"
    class="anyframe.web.springmvc.controller.ForwardController">
    <property name="viewName" value="/dextupload/index.html" />
  </bean>	
</beans>
