<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queryservice PUBLIC "-//ANYFRAME//DTD QUERYSERVICE//EN"
"http://www.anyframejava.org/dtd/anyframe-core-query-mapping-3.1.dtd">

<queryservice>
	<queries>		
		<query id="secfw.volatilization.listTestDupConn" isCamelCase="false">
			<statement>
			<![CDATA[
				SELECT ID
					FROM TEST_DUP_CONNECTION
			]]>
			</statement>
		</query>

		<query id="secfw.volatilization.insertTestDupConn">
			<statement>
			<![CDATA[
				INSERT INTO TEST_DUP_CONNECTION (ID) 
				VALUES (:id)
			]]>
			</statement>
		</query>
									
	</queries>
</queryservice>
