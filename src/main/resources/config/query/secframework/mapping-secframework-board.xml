<queryservice>
	<queries>
		<query id="secfw.board.insertBoard" isCamelCase="false">
			<statement>
			<![CDATA[
				INSERT  INTO  TB_COM_BOARD_NOTICE(
				        NOTICE_ID
				       ,SYS_CD
				       ,BOARD_ID
				       ,UP_NOTICE_ID
				       ,NOTICE_LEVEL
				       ,NOTICE_LEVEL_ORDER
				       ,NOTICE_STATUS_CD
				       ,DIV_CD
				       ,TITLE
				       ,CONTENTS
				       ,CONTENTS_TYPE
				       ,FILE_REF_NO
				       ,IMG_FILE_REF_NO
				       ,REF_CNT
				       ,RECOMMEND_CNT
				       ,OPEN_YN
				       ,SECURITY_YN
				       ,PASSWORD
				       ,ANONYMITY_YN
				       ,ANONYMITY_NM
				       ,HOLD_START_YMD
				       ,HOLD_END_YMD
				       ,URGENCY_YN
				       ,URGENCY_START_YMD
				       ,URGENCY_END_YMD
				       ,DEL_YN
				       ,REG_ID
				       ,REG_NM
				       ,REG_JIKGUP_NM
				       ,REG_DEPT_NM
				       ,REG_DT
				       ,MOD_ID
				       ,MOD_NM
				       ,MOD_JIKGUP_NM
				       ,MOD_DEPT_NM
				       ,MOD_DT )
				VALUES (
				        :vo.notice_id
				       ,:vo.sys_cd
				       ,:vo.board_id
				       ,:vo.up_notice_id
				       ,:vo.notice_level
				       ,:vo.notice_level_order
				       ,:vo.notice_status_cd
				       ,:vo.div_cd
				       ,:vo.title
				       ,:vo.contents
				       ,:vo.contents_type
				       ,:vo.file_ref_no
				       ,:vo.img_file_ref_no
				       ,:vo.ref_cnt
				       ,:vo.recommend_cnt
				       ,:vo.open_yn
				       ,:vo.security_yn
				       ,:vo.password
				       ,:vo.anonymity_yn
				       ,:vo.anonymity_nm
				       ,:vo.hold_start_ymd
				       ,:vo.hold_end_ymd
				       ,:vo.urgency_yn
				       ,:vo.urgency_start_ymd
				       ,:vo.urgency_end_ymd
				       ,:vo.del_yn
				       ,:vo.reg_id
				       ,:vo.reg_nm
				       ,:vo.reg_jikgup_nm
				       ,:vo.reg_dept_nm
				       ,SYSDATE
				       ,:vo.mod_id
				       ,:vo.mod_nm
				       ,:vo.mod_jikgup_nm
				       ,:vo.mod_dept_nm
				       ,SYSDATE)

			]]>
			</statement>
		</query>
		
        
    </queries>
</queryservice>
