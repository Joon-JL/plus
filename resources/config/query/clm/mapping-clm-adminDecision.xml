<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queryservice PUBLIC "-//ANYFRAME//DTD QUERYSERVICE//EN"
"http://www.anyframejava.org/dtd/anyframe-core-query-mapping-3.1.dtd">

<queryservice>
    <queries>
    
    	<!-- 전결규정 목록 조회 : 2011.09.01 : 신남원 -->
		<query id="clm.admin.listDecision" isCamelCase="false">
		  <statement>
		    <![CDATA[
		    SELECT SEQNO
		        ,OPERDIV_CD
		        ,BIZ_CLSFCN
		        ,DEPTH_CLSFCN
		        ,CNCLSNPURPS_BIGCLSFCN
		        ,CNCLSNPURPS_MIDCLSFCN
		        ,APPRVL_CLSFCN
		        ,DCD_CRTRN
		        ,DCD_AUTHMAN
		        ,AGREE_FUNCSTAFF
		        ,AGREE_FUNCSTAFF_NM
		        ,SURTTEAM_AGREE_TRGT
		        ,MNGTSURTOFFICE_AGREE_YN
		        ,NOTE
		        ,RULE_CONT
		        ,REG_DT
		        ,REG_ID
		        ,REG_NM
		        ,MOD_DT
		        ,MOD_ID
		        ,MOD_NM
		        ,DEL_YN
		        ,DEL_DT
		        ,DEL_ID
		        ,DEL_NM
		    FROM TN_CLM_DCD_RULE
		    ]]>
		  </statement>
		</query>
		
    	<!-- 전결규정 등록 : 2011.09.01 : 신남원 -->
		<query id="clm.admin.insertDecision" isCamelCase="false">
		  <statement>
		    <![CDATA[
		    INSERT INTO TN_CLM_DCD_RULE (
		           SEQNO
		               , OPERDIV_CD
		               , BIZ_CLSFCN
		               , DEPTH_CLSFCN
		               , CNCLSNPURPS_BIGCLSFCN
		               , CNCLSNPURPS_MIDCLSFCN
		               , APPRVL_CLSFCN
		               , DCD_CRTRN
		               , DCD_AUTHMAN
		               , AGREE_FUNCSTAFF
		               , AGREE_FUNCSTAFF_NM
		               , SURTTEAM_AGREE_TRGT
		               , MNGTSURTOFFICE_AGREE_YN
		               , NOTE
		               , RULE_CONT
		               , REG_DT
		               , REG_ID
		               , REG_NM
		               , DEL_YN
		        ) VALUES (
		           :vo.seqno
		          ,:vo.operdiv_cd
		          ,:vo.biz_clsfcn
		          ,:vo.depth_clsfcn
		          ,:vo.cnclsnpurps_bigclsfcn
		          ,:vo.cnclsnpurps_midclsfcn
		          ,:vo.apprvl_clsfcn
		          ,:vo.dcd_crtrn
		          ,:vo.dcd_authman
		          ,:vo.agree_funcstaff
		          ,:vo.agree_funcstaff_nm
		          ,:vo.surtteam_agree_trgt
		          ,:vo.mngtsurtoffice_agree_yn
		          ,:vo.note
		          ,:vo.rule_cont
		          ,SYSDATE
		          ,:vo.reg_id
		          ,:vo.reg_nm
		          ,:vo.del_yn
		        )
		    ]]>
		  </statement>
		</query>
		
    	<!-- 전결규정 상세 조회 : 2011.09.01 : 신남원 -->
		<query id="clm.admin.detailDecision" isCamelCase="false">
		  <statement>
		    <![CDATA[
		    SELECT SEQNO
		        ,OPERDIV_CD
		        ,BIZ_CLSFCN
		        ,DEPTH_CLSFCN
		        ,CNCLSNPURPS_BIGCLSFCN
		        ,CNCLSNPURPS_MIDCLSFCN
		        ,APPRVL_CLSFCN
		        ,DCD_CRTRN
		        ,DCD_AUTHMAN
		        ,AGREE_FUNCSTAFF
		        ,AGREE_FUNCSTAFF_NM
		        ,SURTTEAM_AGREE_TRGT
		        ,MNGTSURTOFFICE_AGREE_YN
		        ,NOTE
		        ,RULE_CONT
		        ,REG_DT
		        ,REG_ID
		        ,REG_NM
		        ,MOD_DT
		        ,MOD_ID
		        ,MOD_NM
		        ,DEL_YN
		        ,DEL_DT
		        ,DEL_ID
		        ,DEL_NM
		    FROM TN_CLM_DCD_RULE
		    ]]>
		  </statement>
		</query>
				
    	<!-- 전결규정 수정 : 2011.09.01 : 신남원 -->
		<query id="clm.admin.modifyDecision" isCamelCase="false">
		  <statement>
		    <![CDATA[
		         UPDATE TN_CLM_DCD_RULE
		          SET SEQNO = :vo.seqno
		              , OPERDIV_CD = :vo.operdiv_cd
		              , BIZ_CLSFCN = :vo.biz_clsfcn
		              , DEPTH_CLSFCN = :vo.depth_clsfcn
		              , CNCLSNPURPS_BIGCLSFCN = :vo.cnclsnpurps_bigclsfcn
		              , CNCLSNPURPS_MIDCLSFCN = :vo.cnclsnpurps_midclsfcn
		              , APPRVL_CLSFCN = :vo.apprvl_clsfcn
		              , DCD_CRTRN = :vo.dcd_crtrn
		              , DCD_AUTHMAN = :vo.dcd_authman
		              , AGREE_FUNCSTAFF = :vo.agree_funcstaff
		              , AGREE_FUNCSTAFF_NM = :vo.agree_funcstaff_nm
		              , SURTTEAM_AGREE_TRGT = :vo.surtteam_agree_trgt
		              , MNGTSURTOFFICE_AGREE_YN = :vo.mngtsurtoffice_agree_yn
		              , NOTE = :vo.note
		              , RULE_CONT = :vo.rule_cont
		              , MOD_DT = SYSDATE
		              , MOD_ID = :vo.mod_id
		              , MOD_NM = :vo.mod_nm
		    ]]>
		  </statement>
		</query>
		
    	<!-- 전결규정 삭제 : 2011.09.01 : 신남원 -->
		<query id="clm.admin.deleteDecision" isCamelCase="false">
		  <statement>
		    <![CDATA[
		    DELETE
		    FROM TN_CLM_DCD_RULE
		    ]]>
		  </statement>
		</query>
		
    </queries>
</queryservice>		