<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:config="http://www.anyframejava.org/schema/service"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.anyframejava.org/schema/service
	http://www.anyframejava.org/schema/service/anyframe-core-service-2.0.xsd">
	 
	<!-- 게시판 관리 -->
	<bean name="secfw.idGenerationServiceBBSMaster"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.BBS_MASTER" />
	</bean>
		
	<!-- 게시내역 관리 -->
	<bean name="secfw.idGenerationServiceBBSDetail"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.BBS_DETAIL" />
	</bean>
		
	<!-- 파일관리 -->
	<bean name="secfw.idGenerationServiceFile"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.ATTACH_FILE" />
	</bean>

	<!-- 메뉴 관리 -->
	<bean name="secfw.idGen.menu.menuMngService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.MENU_ID" />
	</bean>

	<!-- 페이지 관리 -->
	<bean name="secfw.idGen.menu.pageMngService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.PAGE_ID" />
	</bean>

	<!-- 역활 관리(역활코드채번) -->
	<bean name="secfw.idGen.auth.roleMngService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.ROLE_CD" />
	</bean>

	<!-- 가이드라인(교육) 관리(일련번호채번) -->
	<bean name="secfw.idGen.library.guidenEduService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="TN_LAS_CONTRACT_GUIDENEDU" />
	</bean>
	<!-- 소송 관리(의뢰번호채번) -->
	<bean name="secfw.idGen.lawsuitmanager.lawSuitService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="TN_LAS_LAWSUIT_REQ" />
	</bean>	
	<!-- 주요사안히스토리(일련번호채번) -->
	<bean name="secfw.idGen.mainproject.mainProjectService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="TN_LAS_MAINMATR_HIST" />
	</bean>	
	<!-- 월간업무(일련번호채번) -->
	<bean name="secfw.idGen.monthduty.statisticsMonthService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="TN_LAS_ETC_MONTHDUTY" />
	</bean>	
	<!-- 주간업무(일련번호채번) -->
	<bean name="secfw.idGen.weekduty.statisticsWeekService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="TN_LAS_ETC_WEEKDUTY" />
	</bean>					
	<!-- 권한 관리(권한코드채번) -->
	<bean name="secfw.idGen.auth.authMngService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.AUTH_CD" />
	</bean>
		
	<!-- MIX Strategy -->
	<bean name="secfw.mixPrefix.Common"
		class="anyframe.core.idgen.impl.strategy.MixPrefix">
		<property name="prefix" value="_" />
		<property name="cipers" value="10" />
		<property name="fillChar" value="0" />
	</bean>
	
	<!-- 게시판 마스터(BOARD_ID 채번) -->
	<bean name="secfw.idGen.auth.boardMngService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.BOARD_MASTER" />
	</bean>
	
	<!-- 게시글(NOTICE_ID 채번) -->
	<bean name="secfw.idGen.auth.boardService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.BOARD_NOTICE" />
	</bean>
	
	<!-- 댓글(APPEND_ID 채번) -->
	<bean name="secfw.idGen.auth.appendService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.BOARD_APPEND" />
	</bean>
	
	<!-- F&A(NOTICE_ID 채번) -->
	<bean name="secfw.idGen.auth.faqService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="SECFW.SHRI_BDFAQ" />
	</bean>
	
	<!-- 법률자문 -->
	<bean name="secfw.idGen.lawconsult.lawConsultService"
		class="anyframe.core.idgen.impl.TableIdGenerationService"
		destroy-method="destroy">
		<property name="dataSource" ref="secfw.dataSource" />
		<property name="strategy" ref="secfw.mixPrefix.Common" />
		<config:configuration block-size="1" table="TB_COM_GENID"
			key-table="TN_LAS_CONSULT" />
	</bean>
	
</beans>
